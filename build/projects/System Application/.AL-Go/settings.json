{
  "$schema": "https://raw.githubusercontent.com/microsoft/AL-Go-Actions/v8.0/.Modules/settings.schema.json",
  "projectName": "System Application and Tools",
  "appFolders": [
    "../../../src/System Application/App",
    "../../../src/Tools/AI Test Toolkit",
    "../../../src/Tools/Test Framework/Test Libraries/*",
    "../../../src/Tools/Test Framework/Test Runner"
  ],
  "doNotRunTests": true,
  "useCompilerFolder": true,
  "doNotPublishApps": true,
  "ConditionalSettings": [
    {
      "branches": [
        "releases/*.[0-5]"
      ],
      "settings": {
        "buildModes": [
          "Strict"
        ]
      }
    }
  ]
}
