{
  "$schema": "https://raw.githubusercontent.com/microsoft/AL-Go/0be3b94b5da6f31f5ac402d6983846b80d387e42/Actions/.Modules/settings.schema.json",
  "projectName": "Apps (W1)",
  "appFolders": [
    "../../../src/Apps/W1/*/App",
    "../../../src/Apps/W1/*/*/App"
  ],
  "testFolders": [
    "../../../src/Apps/W1/*/Test",
    "../../../src/Apps/W1/*/Test Library",
    "../../../src/Apps/W1/*/Demo Data",
    "../../../src/Apps/W1/*/*/Test",
    "../../../src/Apps/W1/*/*/Test Library",
    "../../../src/Apps/W1/*/*/Demo Data"
  ],
  "installOnlyReferencedApps": false,
  "doNotRunPageScriptingTests": true,
  "doNotRunBcptTests": true,
  "buildModes": [
    "CH",
    "CZ",
    "DK",
    "ES",
    "GB",
    "IN",
    "NO",
    "US"
  ],
  "ConditionalSettings": [
    {
      "buildModes": [
        "CH"
      ],
      "settings": {
        "country": "ch"
      }
    },
    {
      "buildModes": [
        "CZ"
      ],
      "settings": {
        "country": "cz",
        "doNotRunTests": true
      }
    },
    {
      "buildModes": [
        "DK"
      ],
      "settings": {
        "country": "dk",
        "doNotRunTests": true
      }
    },
    {
      "buildModes": [
        "ES"
      ],
      "settings": {
        "country": "es"
      }
    },
    {
      "buildModes": [
        "GB"
      ],
      "settings": {
        "country": "gb",
        "doNotRunTests": true
      }
    },
    {
      "buildModes": [
        "IN"
      ],
      "settings": {
        "country": "in"
      }
    },
    {
      "buildModes": [
        "NO"
      ],
      "settings": {
        "country": "no",
        "doNotRunTests": true
      }
    },
    {
      "buildModes": [
        "US"
      ],
      "settings": {
        "country": "us"
      }
    },
    {
      "buildModes": [
        "Clean"
      ],
      "settings": {
        "doNotRunTests": true,
        "useCompilerFolder": true,
        "doNotPublishApps": true
      }
    },
    {
      "branches": [
        "releases/*.[0-5]"
      ],
      "settings": {
        "buildModes": [
          "Strict"
        ]
      }
    }
  ],
  "useProjectDependencies": true
}
