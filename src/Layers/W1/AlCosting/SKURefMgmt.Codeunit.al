    [Scope('OnPrem')]
    procedure Test(LocationCode: Code[10]; ItemNo: Code[20]; VariantCode: Code[10]; UnitCost: Decimal; NewAverageCostLCY: Decimal; NewAverageCostACY: Decimal)
    var
        SKU: Record "Stockkeeping Unit";
        SKURef: Record "SKU Ref.";
        Item: Record Item;
        InvtSetup: Record "Inventory Setup";
        ItemCostMgmt: Codeunit ItemCostManagement;
        AverageCostLCY: Decimal;
        AverageCostACY: Decimal;
    begin
        TableID := DATABASE::"Stockkeeping Unit";
        if not SKU.Get(LocationCode, ItemNo, VariantCode) then
            TestScriptMgmt.TestTextValue('', 'NOT FOUND', 'EXIST', UseCaseNo, TestCaseNo, 0, TableID, IterationNo)
        else begin
            TestScriptMgmt.Increase(NoOfRecords);
            TestScriptMgmt.TestNumberValue(SKU.FieldName("Unit Cost"), SKU."Unit Cost", UnitCost,
              UseCaseNo, TestCaseNo, 0, TableID, IterationNo);
            TestScriptMgmt.Increase(NoOfFields);

            Item.Reset();
            if Item.Get(ItemNo) then begin
                InvtSetup.Get();
                if InvtSetup."Average Cost Calc. Type" = InvtSetup."Average Cost Calc. Type"::"Item & Location & Variant" then begin
                    Item.SetRange("Location Filter", LocationCode);
                    Item.SetRange("Variant Filter", VariantCode);
                end;
            end;
            ItemCostMgmt.CalculateAverageCost(Item, AverageCostLCY, AverageCostACY);
            AverageCostLCY := Round(AverageCostLCY, 0.00001);
            AverageCostACY := Round(AverageCostACY, 0.00001);
            TestScriptMgmt.TestNumberValue(SKURef.FieldName("Average Cost (LCY)"), AverageCostLCY, NewAverageCostLCY,
              UseCaseNo, TestCaseNo, 0, TableID, IterationNo);
            TestScriptMgmt.Increase(NoOfFields);
        end;
    end;

    [Scope('OnPrem')]
    procedure SetNumbers(NewNoOfRecords: Integer; NewNoOfFields: Integer)
    begin
        NoOfRecords := NewNoOfRecords;
        NoOfFields := NewNoOfFields;
    end;

    [Scope('OnPrem')]
    procedure GetNumbers(var NewNoOfRecords: Integer; var NewNoOfFields: Integer)
    begin
        NewNoOfRecords := NoOfRecords;
        NewNoOfFields := NoOfFields;
    end;
