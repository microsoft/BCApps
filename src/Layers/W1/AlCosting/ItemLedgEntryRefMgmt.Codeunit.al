    [Scope('OnPrem')]
    procedure Test(EntryNo: Integer; ItemNo: Code[20]; PostingDate: Date; LocationCode: Code[10]; Quantity: Decimal; RemainingQuantity: Decimal; InvoicedQuantity: Decimal; GlobalDimension1Code: Code[20])
    var
        ILE: Record "Item Ledger Entry";
    begin
        ILE.SetFilter("Entry No.", '>' + Format(Offset));
        // ILE.SETFILTER("Description",'<>VERIFIED');
        ILE.SetRange("Item No.", ItemNo);
        ILE.SetRange("Posting Date", PostingDate);
        ILE.SetRange("Location Code", LocationCode);
        ILE.SetRange(Quantity, Quantity);
        ILE.SetRange("Remaining Quantity", RemainingQuantity);
        ILE.SetRange("Invoiced Quantity", InvoicedQuantity);
        ILE.SetRange("Global Dimension 1 Code", GlobalDimension1Code);
        // EntryNo := EntryNo + Offset;
        // IF NOT ILE.GET(EntryNo) THEN
        if not ILE.FindFirst() then
            TestScriptMgmt.TestTextValue('', 'NOT FOUND', 'EXIST', UseCaseNo, TestCaseNo, EntryNo, TableID, IterationNo)
        else begin
            EntryNo := ILE."Entry No.";

            TestScriptMgmt.Increase(NoOfRecords);
            TestScriptMgmt.TestTextValue(ILE.FieldName("Item No."), ILE."Item No.", ItemNo,
              UseCaseNo, TestCaseNo, EntryNo, TableID, IterationNo);
            TestScriptMgmt.Increase(NoOfFields);
            TestScriptMgmt.TestDateValue(ILE.FieldName("Posting Date"), ILE."Posting Date", PostingDate,
              UseCaseNo, TestCaseNo, EntryNo, TableID, IterationNo);
            TestScriptMgmt.Increase(NoOfFields);
            TestScriptMgmt.TestTextValue(ILE.FieldName("Location Code"), ILE."Location Code", LocationCode,
              UseCaseNo, TestCaseNo, EntryNo, TableID, IterationNo);
            TestScriptMgmt.Increase(NoOfFields);
            TestScriptMgmt.TestNumberValue(ILE.FieldName(Quantity), ILE.Quantity, Quantity,
              UseCaseNo, TestCaseNo, EntryNo, TableID, IterationNo);
            TestScriptMgmt.Increase(NoOfFields);
            TestScriptMgmt.TestNumberValue(ILE.FieldName("Remaining Quantity"), ILE."Remaining Quantity", RemainingQuantity,
              UseCaseNo, TestCaseNo, EntryNo, TableID, IterationNo);
            TestScriptMgmt.Increase(NoOfFields);
            TestScriptMgmt.TestNumberValue(ILE.FieldName("Invoiced Quantity"), ILE."Invoiced Quantity", InvoicedQuantity,
              UseCaseNo, TestCaseNo, EntryNo, TableID, IterationNo);
            TestScriptMgmt.Increase(NoOfFields);
            TestScriptMgmt.TestTextValue(ILE.FieldName("Global Dimension 1 Code"), ILE."Global Dimension 1 Code", GlobalDimension1Code,
              UseCaseNo, TestCaseNo, EntryNo, TableID, IterationNo);
            TestScriptMgmt.Increase(NoOfFields);
            // ILE.VALIDATE("Description",'VERIFIED');
            // ILE.MODIFY(TRUE);
        end;
    end;

