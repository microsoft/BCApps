    [Scope('OnPrem')]
    procedure Test(TableID: Integer; EntryNo: Integer; DimensionCode: Code[20]; DimensionValueCode: Code[20])
    var
        DimSetID: Integer;
        DimSetEntry: Record "Dimension Set Entry";
        GLEntry: Record "G/L Entry";
        ItemLdgEntry: Record "Item Ledger Entry";
        ValueEntry: Record "Value Entry";
    begin
        if (TableID <> 17) and (TableID <> 32) and (TableID <> 5802) then
            Error('COD103455 More table need to be verified. Table ID: %1', TableID);

        if TableID = 17 then begin
            GLEntry.SetRange("Entry No.", EntryNo);
            GLEntry.FindFirst();
            DimSetID := GLEntry."Dimension Set ID";
        end;

        if TableID = 32 then begin
            ItemLdgEntry.SetRange("Entry No.", EntryNo);
            ItemLdgEntry.FindFirst();
            DimSetID := ItemLdgEntry."Dimension Set ID";
        end;

        if TableID = 5802 then begin
            ValueEntry.SetRange("Entry No.", EntryNo);
            ValueEntry.FindFirst();
            DimSetID := ValueEntry."Dimension Set ID";
        end;

        DimSetEntry.SetRange("Dimension Set ID", DimSetID);
        DimSetEntry.SetRange("Dimension Code", DimensionCode);
        if DimSetEntry.FindFirst() then
            TestScriptMgmt.TestTextValue(DimSetEntry.FieldName("Dimension Value Code") + ' ' + DimensionCode,
              DimSetEntry."Dimension Value Code", DimensionValueCode,
              UseCaseNo, TestCaseNo, EntryNo, TableID, IterationNo)
        else
            TestScriptMgmt.TestTextValue('', 'NOT FOUND', 'EXIST', UseCaseNo, TestCaseNo, EntryNo, TableID, IterationNo)
    end;
