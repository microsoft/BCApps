				    )
				)
				
				VAR _YOY =
				    IF (
				        NOT ISBLANK ( ValueCurrentPeriod ) && NOT ISBLANK ( ValuePreviousYear ),
				        ValueCurrentPeriod - ValuePreviousYear
				    )
				
				// -------------------------------------------------
				
				RETURN SWITCH (
				    TRUE,
				    ISINSCOPE ( 'Date'[Calendar Month Year] ), _MOM ,
				    ISINSCOPE ( 'Date'[Calendar Quarter Year] ), _QOQ ,
				    ISINSCOPE ( 'Date'[Calendar Year] ), _YOY
				)

		calculationItem 'POP %' = ```
				VAR ValueCurrentPeriod = SELECTEDMEASURE()
				
				// ------------------------------------------------
				
				VAR ValuePreviousMonth = IF (
				    [ShowValueForDates],
				    CALCULATE (
				        SELECTEDMEASURE(),
				        CALCULATETABLE (
				            DATEADD ( 'Date'[Date], -1, MONTH ),
				            'Date'[DatesWithTransactions] = TRUE
				        )
				    )
				)
				VAR _MOM = 
				    IF (
				        NOT ISBLANK ( ValueCurrentPeriod )
				            && NOT ISBLANK ( ValuePreviousMonth ),
				        ValueCurrentPeriod - ValuePreviousMonth
				    )
				
				VAR _MOM_PERCENT = DIVIDE (
				    _MOM,
				    ValuePreviousMonth
				)
				
				// ------------------------------------------------
				
				VAR ValuePreviousQuarter = IF (
				        [ShowValueForDates],
				        CALCULATE (
				            SELECTEDMEASURE (),
				            CALCULATETABLE (
				                DATEADD ( 'Date'[Date], -1, QUARTER ),
				                'Date'[DatesWithTransactions] = TRUE
				            )
				        )
				    )
				
				VAR _QOQ = IF (
				        NOT ISBLANK ( ValueCurrentPeriod ) && NOT ISBLANK ( ValuePreviousQuarter ),
				        ValueCurrentPeriod - ValuePreviousQuarter
				    )
				
				
				
				VAR _QOQ_PERCENT = DIVIDE (
				    _QOQ,
				    ValuePreviousQuarter
				)
				
				// ------------------------------------------------
				
				VAR ValuePreviousYear =
				    IF (
				        [ShowValueForDates],
				        CALCULATE (
				            SELECTEDMEASURE (),
				            CALCULATETABLE (
				                DATEADD ( 'Date'[Date], -1, YEAR ),
				                'Date'[DatesWithTransactions] = TRUE
				            )
				        )
				    )
				    
				VAR YOY =
				    IF (
				        NOT ISBLANK ( ValueCurrentPeriod ) && NOT ISBLANK ( ValuePreviousYear ),
				        ValueCurrentPeriod - ValuePreviousYear
				    )
				    
				VAR _YOY_PERCENT = DIVIDE ( YOY, ValuePreviousYear )
				
				// ------------------------------------------------
				
				RETURN SWITCH (
				    TRUE,
				    ISINSCOPE ( 'Date'[Calendar Month Year] ), _MOM_PERCENT,
				    ISINSCOPE ( 'Date'[Calendar Quarter Year] ), _QOQ_PERCENT,
				    ISINSCOPE ( 'Date'[Calendar Year] ), _YOY_PERCENT
				)
				```

		calculationItem MATG =
				VAR ValueCurrentPeriod = IF (
				    [ShowValueForDates],
				    CALCULATE (
				        SELECTEDMEASURE(),
				        DATESINPERIOD (
				            'Date'[Date],
				            MAX ( 'Date'[Date] ),
				            -1,
				            YEAR
				        )
				    )
				)
				VAR ValuePreviousPeriod = IF (
				    [ShowValueForDates],
				    CALCULATE (
				        IF (
				            [ShowValueForDates],
				            CALCULATE (
				                SELECTEDMEASURE (),
				                DATESINPERIOD ( 'Date'[Date], MAX ( 'Date'[Date] ), -1, YEAR )
				            )
				        ),
				        DATEADD ( 'Date'[Date], -1, YEAR )
				    )
				)
				RETURN IF (
				        NOT ISBLANK ( ValueCurrentPeriod )
				            && NOT ISBLANK ( ValuePreviousPeriod ),
				        ValueCurrentPeriod - ValuePreviousPeriod
				    )

		calculationItem 'MATG %' =
				VAR ValueCurrentPeriod = IF (
				    [ShowValueForDates],
				    CALCULATE (
				        SELECTEDMEASURE(),
				        DATESINPERIOD (
				            'Date'[Date],
				            MAX ( 'Date'[Date] ),
				            -1,
				            YEAR
				        )
				    )
				)
				VAR ValuePreviousPeriod = IF (
				    [ShowValueForDates],
				    CALCULATE (
				        IF (
				            [ShowValueForDates],
				            CALCULATE (
				                SELECTEDMEASURE (),
				                DATESINPERIOD ( 'Date'[Date], MAX ( 'Date'[Date] ), -1, YEAR )
				            )
				        ),
				        DATEADD ( 'Date'[Date], -1, YEAR )
				    )
				)
				VAR MATG = IF (
				        NOT ISBLANK ( ValueCurrentPeriod )
				            && NOT ISBLANK ( ValuePreviousPeriod ),
				        ValueCurrentPeriod - ValuePreviousPeriod
				    )
				
				VAR PYMAT = IF (
				    [ShowValueForDates],
				    CALCULATE (
				        IF (
				            [ShowValueForDates],
				            CALCULATE (
				                SELECTEDMEASURE (),
				                DATESINPERIOD ( 'Date'[Date], MAX ( 'Date'[Date] ), -1, YEAR )
				            )
				        ),
				        DATEADD ( 'Date'[Date], -1, YEAR )
				    )
				)
				
				RETURN DIVIDE (
				    MATG,
				    PYMAT
				)

