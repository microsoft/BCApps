		calculationItem 'AVG 1Y' =
				VAR Period1Y =
				    CALCULATETABLE (
				        DATESINPERIOD (
				            'Date'[Date],
				            MAX ( 'Date'[Date] ),
				            -1,
				            YEAR
				        ),
				        'Date'[DatesWithTransactions] = TRUE
				    )
				VAR FirstDayWithData =
				    CALCULATE (
				        MIN ( 'G/L Entries'[Posting Date] ),
				        REMOVEFILTERS ()
				    )
				VAR FirstDayInPeriod =
				    MINX ( Period1Y, 'Date'[Date] )
				VAR Result =
				    IF (
				        FirstDayWithData <= FirstDayInPeriod,
				        AVERAGEX (
				            Period1Y,
				            SELECTEDMEASURE()
				        )
				    )
				RETURN
				    Result

