	/// The count of the Document No. column from the Sales table where the Document Type is Order and the Source Type is Sales Order Outstanding.
	measure 'No. of Outstanding Sales Orders' =
			
			CALCULATE
			    (
			        DISTINCTCOUNT('Sales Lines'[Document No.]),
			        'Sales Lines'[Document Type] = "Order",
			        'Sales Lines'[outstandingQtyBase] <> 0
			        )
		formatString: #,0
		displayFolder: _Sales Line Measures\_Counters
		lineageTag: ee2642b3-7b3d-45cb-9de6-ad46485197f0

		changedProperty = IsHidden

	/// The count of the Document No. column from the Sales table where the Document Type is Order and the Source Type is Return Order.
	measure 'No. of Outstanding Return Orders' =
			
			CALCULATE(
			    DISTINCTCOUNT('Sales Lines'[Document No.]),
			    REMOVEFILTERS('Documents'[Document Type]),
			    'Sales Lines'[Document Type] = "Return Order",
			    'Sales Lines'[outstandingQtyBase] <> 0
