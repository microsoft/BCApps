table 'Date Ref'
	isHidden
	lineageTag: 34dcd34d-8050-4c14-8786-8badb8aa258f

	measure ShowValueForDates =
			
			VAR LastDateWithData =
			    CALCULATE (
			        MAX ( 'Item Ledger Entries'[Posting Date] ),
			        REMOVEFILTERS ()
			    )
			VAR FirstDateVisible =
			    MIN ( 'Date Ref'[Date] )
			VAR Result =
			    FirstDateVisible <= LastDateWithData
			RETURN
			    Result
		formatString: """TRUE"";""TRUE"";""FALSE"""
		isHidden
		lineageTag: 6c9e18a5-4b85-4f8c-82a3-201a514a51c6

