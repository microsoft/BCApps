expression ConvertManufacturingStartEndTimes =
		
		
		let
		    Source = (timeInput as nullable datetimezone) as nullable datetime =>
		        let
		            tzDisplayName = Text.Combine(#"Date Table Setup"[timeZoneDisplayName]),
		            result =
		                if timeInput = null or tzDisplayName = null or Text.Trim(tzDisplayName) = "" then
		                    // If input or timezone is null, just return the input as datetime
		                    if timeInput = null then null else DateTime.From(timeInput)
		                else
		                    let
		                        extractUTC = Text.BetweenDelimiters(tzDisplayName, "(", ")"),
		                        removeUTC = Text.AfterDelimiter(extractUTC, "UTC"),
		                        parts = Text.Split(removeUTC, ":"),
		                        switchZone = DateTimeZone.SwitchZone(
		                            timeInput,
		                            Number.FromText(List.First(parts)),
		                            Number.FromText(List.Last(parts))
		                        ),
		                        asDateTime = DateTime.From(switchZone)
		                    in
		                        asDateTime
		        in
		            result
		in
		    Source
