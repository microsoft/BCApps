			            ALLEXCEPT ( 'Date', 'Date'[Day Type], 'Date'[Week Day] ),
			            'Date'[Fiscal YearNumber] = PreviousFiscalYear,
			            'Date'[Fiscal DayOfYearNumber] <= LastDayOfFiscalYearAvailable,
