	measure 'Purchase Amount PY (Weekly)' =
			
			IF (
			    [ShowValueForDates],
			    SUMX (
			        VALUES ( 'Date'[FW YearNumber] ),
			        VAR CurrentFiscalYearNumber = 'Date'[FW YearNumber]
			        VAR DaysSelected =
			            CALCULATETABLE (
			                VALUES ( 'Date'[FW DayOfYearNumber] ),
			                REMOVEFILTERS (
			                    'Date'[Day Type],
			                    'Date'[Week Day],
			                    'Date'[WeekDayNumber]
			                ),
			                'Date'[DateWithPurchases] = TRUE
			            )
			        RETURN
			            CALCULATE (
			                [Purchase (LCY)],
			                'Date'[FW YearNumber] = CurrentFiscalYearNumber - 1,
			                DaysSelected,
			                ALLEXCEPT ( 'Date', 'Date'[Day Type], 'Date'[Week Day] )
			            )
			    )
			)
